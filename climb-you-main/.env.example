# Firebase Configuration
# Copy this file to .env and fill in your Firebase project details

# Firebase Project Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your-api-key-here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789012
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789012:web:abcdef123456789012

# Development Settings
EXPO_PUBLIC_FIREBASE_USE_EMULATOR=true

# AI Service Configuration  
EXPO_PUBLIC_OPENAI_API_KEY=your-openai-api-key
EXPO_PUBLIC_OPENAI_MODEL=gpt-4
EXPO_PUBLIC_OPENAI_TEMPERATURE=0.7
EXPO_PUBLIC_OPENAI_MAX_TOKENS=2000

# Feature Flags
EXPO_PUBLIC_ENABLE_AI_FEATURES=true
EXPO_PUBLIC_DEBUG_API_CALLS=true
EXPO_PUBLIC_USE_MOCK_AI=false
